#include<stdio.h>
#include<string.h>

int main(void)
{
    char str[100];
    
    printf(" enter a string");
    scanf("%s", str);
     int n = strlen(str);
    
    for(int i=0; i < n ;i++)
    {
        if( str[i] >= 'a' && str[i] <= 'z')
        {
            printf("%c", str[i] - 32);
        }
        else
        {
            printf("%c", str[i]);
        }
        
    }
}
