#include<stdio.h>
//#include<cs50.h>
int main(void)
{
    char c='H';
    char c1='I';

    printf("%c\n%c\n",c,c1);
    printf("%d\n", (int) c);

    char str[10]="Happy";//  or In cs50 string s="happy";

    printf("%s\n",str);

    printf("%c  %c\n", (char) str[0],str[1]);
    
    printf("%d  %d\n", (int) str[0],str[1]);
}
